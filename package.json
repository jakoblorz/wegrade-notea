  "scripts": {
    "build": "yarn run build-react && yarn run build-server",
    "start": "node ./bin/index.js",
    "test": "yarn run test-react && yarn run test-server",
    "start-react": "react-app-rewired start",
    "build-react": "react-app-rewired build",
    "test-react": "react-app-rewired test --coverage --watchAll=false --passWithNoTests",
    "test-react-ci": "rm -rf ./coverage && yarn run test-react && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js --verbose",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-server": "babel server --out-dir bin --extensions \".ts,.tsx\" --source-maps inline --copy-files --delete-dir-on-start",
    "test-server": "yarn run build-server && jest run ./bin --coverage --watchAll=false --detectOpenHandles",
    "test-server-ci": "rm -rf ./coverage && yarn run test-server && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js --verbose",
    "chromatic": "npx chromatic --project-token 4zj8ten4s2x --exit-once-uploaded"
  },
